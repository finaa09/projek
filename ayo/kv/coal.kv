            background_color: 1, 1, 1, 0
            foreground_color: rgba(0, 0, 59, 255)
            cursor_color: rgba(0, 0, 59, 255)
            font_size: "14sp"
            cursor_width: "2sp"
            multiline: False
        MDFloatLayout:
            pos_hint: {'center_x': 0.45,'center_y': 0.52}
            size_hint_y: .03
            md_bg_color: rgba(178, 178, 178, 255)

    MDFloatLayout:
        size_hint: .7, .1
