<Coal>:
    MDFloatLayout:
        md_bg_color: 1, 1, 1, 1
    MDIconButton:
        icon: "arrow-left"
        pos_hint: {'center_x': 0.09,'center_y': 0.95}
        on_release:
            root.manager.transition.direction = "right"
            root.manager.current = "standard"
    MDFloatLayout:
        size_hint: .7, .1
        pos_hint: {'center_x': 0.5,'center_y': 0.7}
        TextInput:
            id: set_date
            hint_text: 'Date'
            size_hint_y: .75
            pos_hint: {'center_x': 0.43,'center_y': 0.75}
            background_color: 1, 1, 1, 0
            foreground_color: rgba(0, 0, 59, 255)
            cursor_color: rgba(0, 0, 59, 255)
            font_size: "14sp"
            cursor_width: "2sp"
            multiline: False
            on_focus: root.open_date_picker()
        MDFloatLayout:
            pos_hint: {'center_x': 0.45,'center_y': 0.6}
            size_hint_y: .03
            md_bg_color: rgba(178, 178, 178, 255)

    MDFloatLayout:
        size_hint: .7, .1
