<Daily>:
    MDFloatLayout:
        md_bg_color: 1, 1, 1, 1
    
    #panah
    MDIconButton:
        icon: "arrow-left"
        pos_hint: {'center_x': 0.09,'center_y': 0.95}
        on_release:
            root.manager.transition.direction = "right"
            root.manager.current = "register"

    #date
    MDFloatLayout:
        size_hint: .5, .1
        pos_hint: {'center_x': 0.35,'center_y': 0.8}
        TextInput:
            id: set_date
            hint_text: 'Date'
            size_hint_y: .75
            pos_hint: {'center_x': 0.43,'center_y': 0.75}
            background_color: 1, 1, 1, 0
            foreground_color: rgba(0, 0, 59, 255)
            cursor_color: rgba(0, 0, 59, 255)
            font_size: "13sp"
            cursor_width: "2sp"
            multiline: False
            on_focus: root.open_date_picker()
        MDFloatLayout:
            pos_hint: {'center_x': 0.45,'center_y': 0.6}
            size_hint_y: .03
            md_bg_color: rgba(178, 178, 178, 255)
    
    #composite
    MDRectangleFlatButton:
        text: 'N1'
        font_name: 'SBPoppins'
        size_hint: .3, .1
        font_size: "16sp"
        theme_text_color: "Custom"
        line_color: "blue"
        pos_hint: {'center_x': 0.25,'center_y': 0.7}
        on_release: 
            root.manager.current = "nar1"
    MDRectangleFlatButton:
        text: 'N2'
        font_name: 'SBPoppins'
        size_hint: .3, .1
        font_size: "16sp"
        theme_text_color: "Custom"
        line_color: "blue"
        pos_hint: {'center_x': 0.75,'center_y': 0.7}

    #grave
    MDRectangleFlatButton:
        text: 'Grave N1'
        font_name: 'SBPoppins'
        size_hint: .3, .1
        font_size: "16sp"
        theme_text_color: "Custom"
        line_color: "blue"
        pos_hint: {'center_x': 0.25,'center_y': 0.55}
    MDRectangleFlatButton:
        text: 'Grave N2'
        font_name: 'SBPoppins'
        size_hint: .3, .1
        font_size: "16sp"
        theme_text_color: "Custom"
        line_color: "blue"
        pos_hint: {'center_x': 0.75,'center_y': 0.55}

    #day
    MDRectangleFlatButton:
        text: 'Day N1'
        font_name: 'SBPoppins'
        size_hint: .3, .1
        font_size: "16sp"
        theme_text_color: "Custom"
        line_color: "blue"
        
        pos_hint: {'center_x': 0.25,'center_y': 0.4}
    MDRectangleFlatButton:
        text: 'Day N2'
        font_name: 'SBPoppins'
        size_hint: .3, .1
        font_size: "16sp"
        theme_text_color: "Custom"
        line_color: "blue"
        pos_hint: {'center_x': 0.75,'center_y': 0.4}
    
    #swing
    MDRectangleFlatButton:
        text: 'Swing N1'
        font_name: 'SBPoppins'
        size_hint: .3, .1
        font_size: "16sp"
        theme_text_color: "Custom"
        line_color: "blue"
        pos_hint: {'center_x': 0.25,'center_y': 0.25}
    MDRectangleFlatButton:
        text: 'Swing N2'
        font_name: 'SBPoppins'
