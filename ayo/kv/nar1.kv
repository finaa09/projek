<Nar1>:
    MDFloatLayout:
        md_bg_color: 1, 1, 1, 1
    MDIconButton:
        icon: "arrow-left"
        pos_hint: {'center_x': 0.09,'center_y': 0.95}
        on_release:
            root.manager.transition.direction = "right"
            root.manager.current = "coal"

    MDRectangleFlatButton:
        text: 'Composite NAR1'
        font_name: 'SBPoppins'
        size_hint: .1, .1
        font_size: "16sp"
        theme_text_color: "Custom"
        line_color: "blue"
        pos_hint: {'center_x': 0.5,'center_y': 0.8}
        on_release:
            root.manager.transition.direction = "left"
            root.manager.current = "comp1"
    MDRectangleFlatButton:
        text: 'Grave NAR1'
        font_name: 'SBPoppins'
        size_hint: .1, .1
        font_size: "16sp"
        theme_text_color: "Custom"
        line_color: "blue"
        pos_hint: {'center_x': 0.5,'center_y': 0.65}
        on_release:
            root.manager.transition.direction = "left"
            root.manager.current = "grave1"
    MDRectangleFlatButton:
        text: 'Day NAR1'
        font_name: 'SBPoppins'
        size_hint: .1, .1
        font_size: "16sp"
        theme_text_color: "Custom"
        line_color: "blue"
        pos_hint: {'center_x': 0.5,'center_y': 0.5}
        on_release:
            root.manager.transition.direction = "left"
            root.manager.current = "grave1"
    MDRectangleFlatButton:
        text: 'Swing NAR1'
