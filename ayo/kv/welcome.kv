<Welcome>:
    MDFloatLayout:
        md_bg_color: [1,1,1,1]
    Image:
        source: "assets/image/4385.jpg"
        size_hint: .9, .9
        pos_hint: {'center_x': 0.5,'center_y': 0.68}
    MDLabel:
        text: "H E L L O !"
        halign: "center"
        font_size: "23sp"
        font_name: 'SBPoppins'
        pos_hint: {'center_y': 0.53}
    MDLabel:
        text: "Welcome to this chemical laboratory app"
        font_size: "14sp"
        halign: "center"
        font_name: 'MPoppins'
        size_hint_x: .6
        pos_hint: {'center_x': 0.5, 'center_y': 0.44}
        color: rgba(127, 127, 127, 255)
    MDFillRoundFlatButton:
        text: 'LOGIN'
        font_name: 'SBPoppins'
        size_hint: .6, .065
        pos_hint: {'center_x': 0.5,'center_y': 0.28}
        on_release: 
            root.manager.transition.direction = "left"
            root.manager.current = "login"